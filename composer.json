{
    "name": "popphp/popphp",
    "description": "Pop PHP Framework, a lightweight, robust PHP framework",
    "keywords": [
        "framework",
        "php framework",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "popphp/pop-config" : "^4.0.0",
        "popphp/pop-utils" : "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
