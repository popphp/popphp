{
    "name": "popphp/popphp",
    "description": "Pop PHP Framework, a lightweight, robust PHP framework",
    "keywords": [
        "framework",
        "php framework",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/popphp",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/pop-config" : "^4.0.2",
        "popphp/pop-cookie" : "^4.0.2",
        "popphp/pop-db" : "^6.6.0",
        "popphp/pop-utils" : "^2.1.3",
        "vlucas/phpdotenv": "^5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.3.x-dev"
        }
    }
}
